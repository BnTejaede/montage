{
  "name": "montage",
  "version": "17.0.0",
  "description": "Build your next application with a browser based platform that really gets the web.",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/montagejs/montage.git"
  },
  "main": "montage",
  "overlay": {
    "browser": {
      "main": "core/core",
      "redirects": {
        "montage": "core/core"
      },
      "mappings": {
        "mr": {
          "name": "mr",
          "location": "node_modules/mr"
        },
        "bluebird": {
          "name": "bluebird",
          "location": "node_modules/bluebird"
        }
      }
    },
    "node": {
      "main": "core/core",
      "redirects": {
        "montage": "core/core"
      },
      "mappings": {
        "mr": {
          "name": "mr",
          "location": "node_modules/mr"
        },
        "bluebird": {
          "name": "bluebird",
          "location": "node_modules/bluebird"
        }
      }
    }
  },
  "production": true,
  "dependencies": {
    "collections": "~5.0.x",
    "frb": "~4.0.x",
    "htmlparser2": "~3.0.5",
    "mr": "hthetiot/mr#npmv3",
    "bluebird": "~3.4.7",
    "q-io": "~1.11.0"
  },
  "devDependencies": {
    "jasmine-console-reporter": "^1.2.7",
    "jasmine-core": "^2.5.2",
    "montage-testing": "hthetiot/montage-testing#upgrade"
  },
  "scripts": {
    "test": "node test/node/run-node.js",
    "jsdoc": "node ../montagejs.org/build/jsdoc/jsdoc.js -out $PWD/builds/jsdoc/ montage@npm-link"
  },
  "exclude": [
    "test",
    "tools"
  ]
}
